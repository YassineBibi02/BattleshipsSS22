<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.trying.IpController">
   <children>
      <Label layoutX="125.0" layoutY="58.0" text="Please enter the Ip and Port" textAlignment="CENTER">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button layoutX="398.0" layoutY="319.0" mnemonicParsing="false" onAction="#switchtoPlaying" prefHeight="55.0" prefWidth="116.0" text="Connect" />
      <Button layoutX="83.0" layoutY="319.0" mnemonicParsing="false" onAction="#switchtoMain" prefHeight="55.0" prefWidth="116.0" text="Return" />
      <Label layoutX="283.0" layoutY="123.0" text="IP:" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="276.0" layoutY="190.0" text="Port:" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="iP_Field" layoutX="177.0" layoutY="152.0" prefHeight="25.0" prefWidth="243.0" text="localhost" />
      <TextField fx:id="Port_Field" layoutX="177.0" layoutY="218.0" prefHeight="25.0" prefWidth="243.0" promptText="6969" />
      <Button layoutX="241.0" layoutY="319.0" mnemonicParsing="false" onAction="#switchtoHosting" prefHeight="55.0" prefWidth="116.0" text="Host" />
      <Label fx:id="Feedback" alignment="CENTER" layoutX="177.0" layoutY="255.0" prefHeight="22.0" prefWidth="243.0" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
